source :rubygems

gem 'mysql2'
gem 'jquery-rails'
gem 'jasminerice'

gem 'multi_json', '>= 1.3.4'

# Gems used only for assets and not required
# in production environments by default.
group :assets do
  gem 'sass-rails'
  gem 'coffee-rails'
  gem 'uglifier'
end

group :test do
  gem 'rspec'
  gem 'rspec-rails'
  gem 'cucumber'
  gem 'cucumber-rails'
  gem 'capybara'
  gem 'shoulda-matchers'
  gem 'database_cleaner'
  gem 'selenium-webdriver',   '>= 2.21.2'
end

# Specify your gem's dependencies in ..gemspec
gemspec
